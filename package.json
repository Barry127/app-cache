{
  "name": "app-cache",
  "version": "0.0.1",
  "description": "Cache for node apps",
  "main": "index.js",
  "scripts": {
    "lint": "reset && eslint . || true",
    "lint:watch": "onchange \"**/*.js\" -- npm run lint",
    "test": "mocha --recursive \"**/*.spec.js\"",
    "test:coverage": "istanbul cover _mocha \"**/*.spec.js\" -- -R spec",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
